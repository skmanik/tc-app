  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "firebase-admin": "^6.0.0",
    "if-env": "^1.0.4"
  }
}
